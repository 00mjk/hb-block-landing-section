{
	"apiVersion": 2,
	"name": "hb/landing-section",
	"version": "0.1.0",
	"title": "Hb Block: Landing Section",
	"category": "text",
	"icon": "smiley",
	"description": "A full width grid template for landing page sections.",
	"keywords": [ "landing page", "section", "full width" ],
	"textdomain": "hb-landing-section",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"gridColumnStart": {
			"enum": [ "oneone-l", "threetwo-l", "sixteennine-l", "full-l" ],
			"source": "attribute",
			"selector": "section",
			"attribute": "style.gridColumnStart",
			"default": "oneone-l"
		},
		"gridColumnEnd": {
			"enum": [ "oneone-r", "threetwo-r", "sixteennine-r", "full-r" ],
			"source": "attribute",
			"selector": "section",
			"attribute": "style.gridColumnEnd",
			"default": "oneone-r"
		},
		"gridColumn": {
			"enum": [ "oneone", "threetwo", "sixteennine", "full", "custom" ],
			"source": "attribute",
			"selector": "section",
			"attribute": "style.gridColumn",
			"default": "oneone"
		},
		"minHeight": {
			"type": "string",
			"source": "attribute",
			"selector": ".hb__landingSection_content",
			"attribute": "style.minHeight",
			"default": "60px"
		},
		"inlineGridStyleTest": {
			"type": "array",
			"source": "attribute",
			"selector": ".hb__landingSection_content",
			"attribute": "style"
		}
	},
	"supports": {
		"anchor": true,
		"className": false,
		"customClassName": true,
		"color": {
			"gradients": true,
			"link": true
		}
	}
}