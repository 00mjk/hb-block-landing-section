{
	"apiVersion": 2,
	"name": "hb/landing-section",
	"version": "0.1.0",
	"title": "Hb Block: Landing Section",
	"category": "text",
	"icon": "smiley",
	"description": "A full width grid template for landing page sections.",
	"keywords": [ "landing page", "section", "full width" ],
	"textdomain": "hb-landing-section",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"contentDivClasses": {
			"type": "string",
			"source": "attribute",
			"selector": ".hb__landingSection_content",
			"attribute": "class",
			"default": "hb__landingSection_content oneone-l oneone-r"
		},
		"selectControlStart": {
			"type": "string",
			"enum": [ "oneone-l", "threetwo-l", "sixteennine-l", "full-l" ],
			"default": "oneone-l"
		},
		"selectControlEnd": {
			"type": "string",
			"enum": [ "oneone-r", "threetwo-r", "sixteennine-r", "full-r"  ],
			"default": "oneone-r"
		},
		"selectControlBoth": {
			"type": "string",
			"enum": [ "oneone", "threetwo", "sixteennine", "full", "Custom" ],
			"default": "oneone"
		}
	},
	"supports": {
		"anchor": true,
		"className": false,
		"customClassName": true,
		"color": {
			"gradients": true,
			"link": true
		}
	}
}